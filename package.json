{
  "name": "beer-tracker",
  "version": "1.0.0",
  "description": "Web scraping tool voor drankprijzen bij Belgische supermarkten",
  "main": "dist/index.js",
  "scripts": {
    "build": "tsc",
    "start": "node dist/index.js",
    "dev": "ts-node src/index.ts",
    "dev:debug": "cross-env DEBUG=true ts-node src/index.ts",
    "scrape": "npm run build && npm start"
  },
  "keywords": [
    "scraping",
    "price-comparison",
    "supermarkets",
    "belgium"
  ],
  "author": "",
  "license": "MIT",
  "dependencies": {
    "playwright": "^1.40.0",
    "express": "^4.18.2",
    "cors": "^2.8.5",
    "ws": "^8.14.2"
  },
  "devDependencies": {
    "@types/node": "^20.10.0",
    "@types/express": "^4.17.21",
    "@types/cors": "^2.8.17",
    "@types/ws": "^8.5.10",
    "cross-env": "^7.0.3",
    "ts-node": "^10.9.2",
    "typescript": "^5.3.3"
  }
}

